# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'roomate' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for roomate

  # Facebook
pod ‘Bolts’
pod ‘FBSDKCoreKit' 
pod ‘FBSDKLoginKit'
pod ‘FBSDKShareKit'

  # Payment
pod 'PayPal-iOS-SDK'

  # Push Notifications
pod ‘OneSignal’

  target 'roomateTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
